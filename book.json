{
    "plugins": [
        "code",
        "custom-favicon",
        "theme-default",
        "splitter",
        "anchor-navigation-ex",
        "insert-logo"
    ],
    "pluginsConfig": {
        "code": {
            "copyButtons": true
        },
        "favicon": "image/wpie.ico",
        "theme-default": {
            "showLevel": false
        },
        "anchor-navigation-ex": {
            "showLevel": true,
            "showGoTop": true
        },
        "insert-logo": {
            "url": "image/wpie.png",
            "style": "background: none; max-height: 30px; min-height: 30px"
        }
    }
}

